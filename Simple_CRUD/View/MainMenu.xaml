<Window x:Class="Simple_CRUD.View.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Simple_CRUD.View"
        Language="ru-RU"
        mc:Ignorable="d"
        Title="Главное меню" Height="450" Width="800">
    <UniformGrid Rows="2">
        <Button Command="{Binding OpenEngineCommand}" Content="Игровые движки"/>
        <Button Command="{Binding OpenGameCommand}" Content="Игры"/>
    </UniformGrid>
</Window>
